    "from urllib import request\n",
    "\n",
    "r = request.Request('http://localhost:8080/')\n",
    "r.add_header(\n",
    "    'User-agent',\n",
    "    'PyMOTW (https://pymotw.com/)',\n",
    ")\n",
    "\n",
    "response = request.urlopen(r)\n",
    "data = response.read().decode('utf-8')\n",
    "print(data)"
